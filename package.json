{
  "name": "marine-server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "nodemon",
    "build": "tsc",
    "generate": "ts-node src/generate-module.ts",
    "postinstall": "npx prisma generate && npx prisma db push",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint:fix": "npm run eslint:check -- --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint-staged": "lint-staged",
    "lint": "npm run eslint:check && npm run prettier:check",
    "prepare": "husky install",
    "precommit": "npm run lint-staged",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,*rc}",
    "seed": "ts-node prisma/seed.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix --ext .ts --config eslint.config.mjs"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/disposable-email-domains": "^1.0.6",
    "@types/express": "^5.0.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "prisma": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.806.0",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "disposable-email-domains": "^1.0.62",
    "dotenv": "^16.5.0",
    "eslint-plugin-import": "^2.32.0",
    "express": "^5.1.0",
    "firebase-admin": "^13.6.0",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "nodemon": "^3.1.10",
    "socket.io": "^4.8.1",
    "zod": "^3.24.3"
  }
}
